   "source": [
    "import numpy as np \n",
    "from scipy import optimize \n",
    "\n",
    "f = lambda x: np.cos(x) - x \n",
    "r = optimize.fsolve(f, -2) \n",
    "print(\"r =\", r) \n",
    "\n",
    "# Verifikasi solusi adalah akar\n",
    "hasil = f(r)\n",
    "print(\"hasil=\", hasil)"
   ]
  }
