   "source": [
    "# Integrasi Numerik dengan Metode Trapesium \n",
    "import numpy as np \n",
    "\n",
    "a = 0 \n",
    "b = np.pi \n",
    "n = 11 \n",
    "h = (b - a) / (n - 1) \n",
    "x = np.linspace(a, b, n) \n",
    "f = np.sin(x) \n",
    " \n",
    "I_trap = (h/2)*(f[0] + 2 * sum(f[1:n-1]) + f[n-1]) \n",
    "err_trap = 2 - I_trap  \n",
    "\n",
    "print(I_trap) \n",
    "print(err_trap) "
   ]
  }
 ],
